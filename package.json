{
  "name": "poker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ssr:start:prod" : "export NODE_ENV=production; PORT=7100 node ./dist/server-build/bin/www",
    "ssr:start:dev": "concurrently --kill-others 'npm run dev:start' \"node webpack.server.js\" \"gulp css:watch\"",
    "test": "cross-env NODE_ENV=test cross-env NODE_PATH=./app mocha --compilers js:babel-register -r app/spec/support/setup.mocha.js --recursive 'app/spec/**/*.test.js' -w",
    "test:ci": "cross-env NODE_ENV=test cross-env NODE_PATH=./app mocha --compilers js:babel-register -r app/spec/support/setup.mocha.js --recursive 'app/spec/**/*.test.js'",
    "webpack": "webpack",
    "ssr:build": "rimraf dist/ && npm run webpack && gulp build",
    "ssr:webserver" : "concurrently --kill-others  \"export NODE_ENV=development; nodemon bin/www\" \"node webpack.server.js\" \"gulp css:watch\"",
    "start": "node ./bin/www",
    "dev:start": "export NODE_ENV=development; PORT=7100 nodemon ./bin/www",
    "webserver": "webpack-dev-server --port 7000 --progress --colors --inline --hot --host 0.0.0.0 --content-base build/",
    "dev": "concurrently 'npm run dev:start' 'npm run webserver'",
    "debug": "export NODE_ENV=development; PORT=7100 node --inspect --debug-brk ./bin/www",
    "stats": "webpack --profile --json > stats.json",
    "start:webserver": "webpack-dev-server",
    "deploy": "gh-pages -d build",
    "build": "rm -rf build/* && webpack -p",
    "test": "karma start",
    "test:tdd": "npm run test -- --auto-watch --no-single-run",
    "test:lint": "eslint ./app ./tests --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache"
  },
